            <collision>
                <origin rpy="0 0 0" xyz="0 0 ${connector_l/2}"/>
                <geometry>
                    <box size="${rdm_l} ${rdm_l} ${connector_l}"/>
                </geometry>
            </collision>
        </link>

        <joint name="${prefix}fl_hip_pitch" type="revolute">
