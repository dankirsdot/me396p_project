        <link name="${prefix}rr_hip_roll_actuator">
            <xacro:inertia_cylinder mass="1" length="${rdm_l}" radius="${rdm_r}">
                <origin rpy="0 0 0" xyz="0 0 ${rdm_l/2}"/>
            </xacro:inertia_cylinder>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 ${rdm_l/2}"/>
                <geometry>
                    <cylinder length="${rdm_l}" radius="${rdm_r}"/>
