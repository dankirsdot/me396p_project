        </link>

        <joint name="${prefix}rr_hip_to_knee" type="fixed">
            <parent link="${prefix}rr_hip_pitch_actuator"/>
            <child link="${prefix}rr_knee_pitch_connector"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
