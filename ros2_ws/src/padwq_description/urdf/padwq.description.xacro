            <parent link="${prefix}rr_hip_pitch_connector"/>
            <child link="${prefix}rr_hip_pitch_actuator"/>
            <origin rpy="${pi/2} 0 0" xyz="0 -${rdm_l/2} -0.068"/>
            <limit effort="1000.0" lower="${-51 * deg2rad}" upper="${93 * deg2rad}" velocity="0.7"/>
