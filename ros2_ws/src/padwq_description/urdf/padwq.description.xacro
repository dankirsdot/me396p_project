                </geometry>
                <material name="white"/>
            </visual>
            <collision>
                <origin rpy="0 0 0" xyz="0.11 0 0"/>
                <geometry>
                    <box size="0.22 0.04 0.04"/>
                </geometry>
            </collision>
        </link>

        <joint name="${prefix}rr_knee_pitch" type="revolute">
            <axis xyz="0 0 1"/>
            <parent link="${prefix}rr_upper_link"/>
            <child link="${prefix}rr_under_link"/>
            <origin rpy="0 0 0" xyz="0.22 0 0"/>
