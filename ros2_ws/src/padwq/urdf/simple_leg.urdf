<?xml version="1.0" ?>
<robot name="single_leg_robot">
    <material name="blue">
        <color rgba="0 0 1 1.000"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1.000"/>
    </material>
    <material name="silver">
        <color rgba="0.700 0.700 0.700 1.000"/>
    </material>

    <link name="base">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="100"/>
            <inertia ixx="8.3542" ixy="0" ixz="0" iyy="8.3542" iyz="0" izz="16.6667"/>
        </inertial>
        <visual>
            <geometry>
                <box size="1 0.5 0.05"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <geometry>
                <box size="1 0.5 0.05"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_base_link" type="fixed">
        <parent link="base_link"/>
        <child link="base"/>
        <origin rpy="0 0 0" xyz="0 -0.225 -0.5"/>
    </joint>

    <link name="base_link">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="50"/>
            <inertia ixx="8.3542" ixy="0" ixz="0" iyy="8.3542" iyz="0" izz="0.0417"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.05 0.05 1"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.05 0.05 1"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_joint" type="prismatic">
        <axis xyz="0 0 1"/>
        <parent link="slide_link"/>
        <child link="base_link"/>
        <limit effort="1000.0" lower="-0.5" upper="0.425" velocity="0.25"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>

    <link name="slide_link">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="10"/>
            <inertia ixx="0.0167" ixy="0" ixz="0" iyy="0.0167" iyz="0" izz="0.0167"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
        </collision>
    </link>

    <joint name="upper_joint" type="continuous">
        <axis xyz="0 1 0"/>
        <parent link="slide_link"/>
        <child link="upper_link"/>
        <origin rpy="0 1.5708 0" xyz="0 0.07 0"/>
    </joint>

    <link name="upper_link">
        <inertial>
            <origin rpy="0 0 0" xyz="0.11 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.000066667" ixy="0" ixz="0" iyy="0.016166667" iyz="0" izz="0.016166667"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0.11 0 0"/>
            <geometry>
                <box size="0.22 0.04 0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0.11 0 0"/>
            <geometry>
                <box size="0.22 0.04 0.04"/>
            </geometry>
        </collision>
    </link>

    <joint name="under_joint" type="continuous">
        <axis xyz="0 1 0"/>
        <parent link="upper_link"/>
        <child link="under_link"/>
        <origin rpy="0 0 0" xyz="0.22 0 0"/>
    </joint>

    <link name="under_link">
        <inertial>
            <origin rpy="0 0 0" xyz="0.11 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.000066667" ixy="0" ixz="0" iyy="0.016166667" iyz="0" izz="0.016166667"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0.11 0 0"/>
            <geometry>
                <box size="0.22 0.04 0.04"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0.11 0 0"/>
            <geometry>
                <box size="0.22 0.04 0.04"/>
            </geometry>
        </collision>
    </link>

    <joint name="foot" type="fixed">
        <parent link="under_link"/>
        <child link="foot"/>
        <origin rpy="0 0 0" xyz="0.22 0 0"/>
    </joint>

    <link name="foot">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>
    </link>

</robot>
